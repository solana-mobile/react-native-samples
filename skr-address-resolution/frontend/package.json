    "react-native-toast-message": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@types/react": "~19.1.17",
    "eslint": "^9.33.0",
    "eslint-config-expo": "~10.0.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  },
